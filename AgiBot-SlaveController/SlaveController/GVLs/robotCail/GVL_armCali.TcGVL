	//create calibration file
	g_createCaliFile :ARRAY [1..4] OF BOOL;
	g_createCaliFileTrigger :ARRAY [1..4] OF BOOL;
	
	//calibration data write file
	g_caliDataWrite :ARRAY [1..4] OF BOOL;
	g_caliDataWriteTrigger :ARRAY [1..4] OF BOOL;
	
	//powerbox calibration
	g_powerBoxCali : ARRAY [1..4] OF BOOL;
