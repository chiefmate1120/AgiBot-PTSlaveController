  <GVL Name="GVL_armCali" Id="{4b25f0e4-26a3-463d-bd1e-f771cb3aeb62}">
    <Declaration><![CDATA[{attribute 'qualified_only'}
VAR_GLOBAL
	//arm enter calibration mode,state machine jump to ArmCali
	g_armEnterCaliMode :Vec4b;
	
	//joint calibration state,  this joint will go into current mode
	g_jointIntoCaliState :ARRAY [1..4] OF  vec8b;
	
	//joint link encoder frist position calibration
	g_linkEncFristPosCali :ARRAY [1..4] OF  vec8b;
	g_linkEncFristCaliTrigger :ARRAY [1..4] OF Vec8b;
	
	//joint link encoder second position calibration
	g_linkEncSecondPosCali :ARRAY [1..4] OF  vec8b;
	g_linkEncSecondCaliTrigger :ARRAY [1..4] OF Vec8b;
	
	//joint link encoder home position calibration
	g_linkEncHomePosCali :ARRAY [1..4] OF Vec8b;
	g_linkEncHomePosCaliTrigger :ARRAY [1..4] OF Vec8b;
	
