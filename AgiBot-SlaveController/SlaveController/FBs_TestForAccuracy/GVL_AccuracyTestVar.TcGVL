<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.11">
  <GVL Name="GVL_AccuracyTestVar" Id="{45581a22-0baa-4057-ac9f-52096dc1a176}">
    <Declaration><![CDATA[{attribute 'qualified_only'}
VAR_GLOBAL
	//enter precision test mode
	g_enterPrecisionTestMode:ARRAY[1..g_sArmNum] OF BOOL;
	
	//start motion flag
	g_startMotion:ARRAY[1..g_sArmNum] OF BOOL;
	
	//control Mode
	g_controlMode:ARRAY[1..g_sArmNum] OF INT;
		
	//next step button
	g_nextStep:ARRAY[1..g_sArmNum] OF BOOL;
	
	//current step
	g_curStep:ARRAY[1..g_sArmNum] OF INT;
	
	//current test point id
	g_testPointId:ARRAY[1..g_sArmNum] OF INT;
	
	//current cycle id
	g_testCycleId:ARRAY[1..g_sArmNum] OF INT;
	
	//origin joint position
	g_originJntPos:ARRAY[1..g_sArmNum] OF Vec6d;
	
	//Target joint position for control mode 1---Joint space single point motion
	g_targetJntPos:ARRAY[1..g_sArmNum] OF Vec6d;
	
	//joint motion direction for control mode 2---Continuous joint space motion
	g_jntMotionDir:ARRAY[1..g_sArmNum] OF Vec6i;
	
	//joint motion velocity for control mode 2---Continuous joint space motion
	g_jntVel:ARRAY[1..g_sArmNum] OF Vec6d;
	
	//target space position for control mode 3---Single point motion in operating space
	g_targetSpatialPos:ARRAY[1..g_sArmNum] OF ST_Frame;
	
	//target space motion direction for control mode 4---Continuous motion in operating space
	g_spatialMotionDir:ARRAY[1..g_sArmNum] OF Vec6i;
	
	//target space velocity for control mode 4---Continuous motion in operating space
	g_spatialVel:ARRAY[1..g_sArmNum] OF Vec6d;
	
	//actual joint position
	g_actDHJntPos:ARRAY[1..g_sArmNum] OF Vec6d;
		
	//drag joint flags
	g_dragJntFlag:ARRAY[1..g_sArmNum] OF Vec6i;
	
	//write data
	g_writeData:ARRAY[1..g_mArmNum] OF BOOL;
	
	//slave arm spatial pose frame
	g_frontPose:ARRAY[1..4] OF ST_Frame;
	g_endPose:ARRAY[1..4] OF ST_Frame;
	g_slaveArmPose:ARRAY[1..4] OF ST_Frame;
	
	//ui signal
	g_uiArmMode:ARRAY[1..g_sArmNum,0..gvl_accuracyTest.g_controlModeNum] OF BOOL;
END_VAR]]></Declaration>
  </GVL>
</TcPlcObject>